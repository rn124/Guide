<kits>
  <kit name="spawn">
    <item slot="2" enchantment="dig speed:3">diamond pickaxe</item>
  </kit>
  <kit name="red" parents="spawn">
    <chestplate color="ff5555" bind="true">leather chestplate</chestplate>
  </kit>
  <kit name="blue" parents="spawn">
    <chestplate color="5555ff" bind="true">leather chestplate</chestplate>
  </kit>
  <kit name="haste">
    <potion duration="30">fast digging</potion>
  </kit>
  <kit name="haste2">
    <potion duration="30" amplifier="1">fast digging</potion>
  </kit>
</kits>

<filters>
  <filter name="get-effect-red">
    <all>
      <team>red</team>
      <elapsed min="10m" max="20m"/>
    </all>
  </filter>
  <filter name="get-effect-red2">
    <all>
      <team>red</team>
      <elapsed min="20m" max="30m"/>
    </all>
  </filter>
  <filter name="get-effect-blue">
    <all>
      <team>blue</team>
      <elapsed min="10m" max="20m"/>
    </all>
  </filter>
  <filter name="get-effect-blue2">
    <all>
      <team>blue</team>
      <elapsed min="20m" max="30m"/>
    </all>
  </filter>
</filters>

<regions>
  <apply kit="haste" filter="get-effect-red">
    <rectangle min="3,-3" max="9,-9"/>
  </apply>
  <apply kit="haste2" filter="get-effect-red2">
    <rectangle min="3,-3" max="9,-9"/>
  </apply>
  <apply kit="haste" filter="get-effect-blue">
    <rectangle min="-3,3" max="-9,9"/>
  </apply>
  <apply kit="haste2" filter="get-effect-blue2">
    <rectangle min="-3,3" max="-9,9"/>
  </apply>
</regions>
